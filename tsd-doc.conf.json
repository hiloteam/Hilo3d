{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include":"src",
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"    
    },

    "plugins": [ "./node_modules/@hilo/tsd-jsdoc/dist/plugin" ],
   
    "opts":{
        "template":"./node_modules/@hilo/tsd-jsdoc/dist",
        "encoding":"utf8",
        "destination":"types",
        "outFile":"index.d.ts",
        "recurse":true
    }
}